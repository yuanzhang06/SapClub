{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "com.sap",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.32.0"
		}
	},

	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},

	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "com.sap.view.View1",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sap.i18n.i18n"
				}
			}
		},
		"routing" : {
			"config" : {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.sap.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes" : [
			    {
					"pattern" : "",
					"name" : "logon",
					"target" : "home"
				},
				{
					"pattern" : "TEST",
					"name" : "logonPage",
					"target" : "masterDetail"
				},	
				{
					"pattern" : "test",
	        		"name" : "test",
	        		"target" : "masterDetail" 
				}
			],
			"targets" : {
	          	"home": {
	             	"viewName": "Logon",
	             	"viewLevel" : 1
	          	},
	          	"herUser": {
	          		"viewName" : "CustomerUser",
	          		"viewLevel" : 2
	          	},
              	"notFound": {
                 	"viewName": "NotFound",
                 	"transition": "show",
                 	"viewLevel" : 2
              	},
	          	"masterDetail": {
	             	"viewName": "clubDetail"
	          	}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}